if defined("configure_system_libs")
  configure_system_libs()
end

if SYSTEM_LIBS contains "POSTGRES95"

import_sys_lib ("POSTGRES95")
import ("libm3")

Interface ("PQ")
Interface ("PQcomm")
Interface ("Postgres")

implementation ("PQ")

if equal (TARGET, "ALPHA_OSF")
  % missing libpq.a => can't build local .so file...
  build_standalone ()
end

Library ("postgres95")

end