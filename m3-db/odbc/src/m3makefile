if SYSTEM_LIBS contains "ODBC"

    import("libm3")
    import_sys_lib ("ODBC")

    if equal (TARGET, "NT386")
        include_dir ("WIN32")
    else
        include_dir (OS_TYPE)
    end

    Library("m3odbc")
end
