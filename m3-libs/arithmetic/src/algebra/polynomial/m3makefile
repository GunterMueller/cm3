import("libm3")

template("polynomial")


generic_module("Polynomial")

generic_module("PolynomialFast")
generic_module("PolynomialBasic")
Polynomial("Real",     "Fast", "Real",     "Basic")
Polynomial("LongReal", "Fast", "LongReal", "Basic")
Polynomial("Extended", "Fast", "Extended", "Basic")

Polynomial("RealComplex",        "Basic", "RealComplex",        "Fast")
Polynomial("LongRealComplex",    "Basic", "LongRealComplex",    "Fast")
Polynomial("ExtendedComplex",    "Basic", "ExtendedComplex",    "Fast")
Polynomial("Integer32",          "Basic", "Integer32",          "Basic")
Polynomial("BigInteger",         "Basic", "BigInteger",         "Basic")
Polynomial("BigIntegerFraction", "Basic", "BigIntegerFraction", "Basic")

generic_module("PolynomialFmtLex")
PolynomialFmtLex("Real",               "Real",               "Basic")
PolynomialFmtLex("LongReal",           "LongReal",           "Basic")
PolynomialFmtLex("Extended",           "Extended",           "Basic")
PolynomialFmtLex("LongRealComplex",    "LongRealComplex",    "Fast")
PolynomialFmtLex("Integer32",          "Integer32",          "Basic")
PolynomialFmtLex("BigInteger",         "BigInteger",         "Basic")
PolynomialFmtLex("BigIntegerFraction", "BigIntegerFraction", "Basic")
