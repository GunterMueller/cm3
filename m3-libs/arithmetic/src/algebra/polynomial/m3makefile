import("libm3")

template("polynomial")


generic_module("PolynomialBasic")
PolynomialBasic("Real",               "RealVectorBasic",               "RealBasic")
PolynomialBasic("LongReal",           "LongRealVectorBasic",           "LongRealBasic")
PolynomialBasic("Extended",           "ExtendedVectorBasic",           "ExtendedBasic")
PolynomialBasic("RealComplex",        "RealComplexVectorBasic",        "RealComplexBasic")
PolynomialBasic("LongRealComplex",    "LongRealComplexVectorBasic",    "LongRealComplexBasic")
PolynomialBasic("ExtendedComplex",    "ExtendedComplexVectorBasic",    "ExtendedComplexBasic")
PolynomialBasic("Integer32",          "Integer32VectorBasic",          "Integer32Basic")
PolynomialBasic("BigInteger",         "BigIntegerVectorBasic",         "BigIntegerBasic")
PolynomialBasic("BigIntegerFraction", "BigIntegerFractionVectorBasic", "BigIntegerFractionBasic")

generic_module("PolynomialFast")
PolynomialFast("Real",     "RealVectorFast",     "RealBasic")
PolynomialFast("LongReal", "LongRealVectorFast", "LongRealBasic")
PolynomialFast("Extended", "ExtendedVectorFast", "ExtendedBasic")

generic_module("PolynomialFmtLex")
PolynomialFmtLex("Real",     "RealPolynomialFast",     "RealFmtLex")
PolynomialFmtLex("LongReal", "LongRealPolynomialFast", "LongRealFmtLex")
PolynomialFmtLex("Extended", "ExtendedPolynomialFast", "ExtendedFmtLex")
PolynomialFmtLex("LongRealComplex",    "LongRealComplexPolynomialBasic",    "LongRealComplexFmtLex")
PolynomialFmtLex("Integer32",          "Integer32PolynomialBasic",          "Integer32FmtLex")
PolynomialFmtLex("BigInteger",         "BigIntegerPolynomialBasic",         "BigIntegerFmtLex")
PolynomialFmtLex("BigIntegerFraction", "BigIntegerFractionPolynomialBasic", "BigIntegerFractionFmtLex")
