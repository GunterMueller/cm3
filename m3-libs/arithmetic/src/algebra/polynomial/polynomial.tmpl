%
%  Quake functions to instantiate operations on generic polynomials.
%

readonly proc PolynomialBasic (nm, field) is
  local basic = nm & "PolynomialBasic"
  build_generic_intf (basic, "PolynomialBasic", [field], VISIBLE)
  build_generic_impl (basic, "PolynomialBasic", [field])
end

readonly proc PolynomialFast (nm, field) is
  local basic = nm & "PolynomialFast"
  build_generic_intf (basic, "PolynomialFast", [field], VISIBLE)
  build_generic_impl (basic, "PolynomialFast", [field])
end

readonly proc PolynomialFmtLex (nm, poly, rfmt) is
  local basic = nm & "PolynomialFmtLex"
  build_generic_intf (basic, "PolynomialFmtLex", [poly, rfmt], VISIBLE)
  build_generic_impl (basic, "PolynomialFmtLex", [      rfmt])
end
