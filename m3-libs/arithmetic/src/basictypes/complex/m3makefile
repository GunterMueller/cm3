import("libm3")

template("complex")


generic_module("ComplexBasic")
ComplexBasic("Real",       "Real",       "Basic")
ComplexBasic("LongReal",   "LongReal",   "Basic")
ComplexBasic("Extended",   "Extended",   "Basic")
ComplexBasic("Integer32",  "Integer32",  "Basic")
ComplexBasic("BigInteger", "BigInteger", "Basic")

generic_module("ComplexFast")
ComplexFast("Real",     "Real",     "Basic")
ComplexFast("LongReal", "LongReal", "Basic")
ComplexFast("Extended", "Extended", "Basic")


generic_module("ComplexTrans")
ComplexTrans("Real",     "Real",     "Basic", "Fast")
ComplexTrans("LongReal", "LongReal", "Basic", "Fast")
ComplexTrans("Extended", "Extended", "Basic", "Fast")

generic_module("ComplexFmtLex")
ComplexFmtLex("Real",	    "Real",       "Basic", "Fast")
ComplexFmtLex("LongReal",   "LongReal",   "Basic", "Fast")
ComplexFmtLex("Extended",   "Extended",   "Basic", "Fast")
ComplexFmtLex("Integer32",  "Integer32",  "Basic", "Basic")
ComplexFmtLex("BigInteger", "BigInteger", "Basic", "Basic")
