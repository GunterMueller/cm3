import("libm3")

template("complex")


generic_module("ComplexBasic")
ComplexBasic("Real",       "RealBasic")
ComplexBasic("LongReal",   "LongRealBasic")
ComplexBasic("Extended",   "ExtendedBasic")
ComplexBasic("Integer32",  "Integer32Basic")
ComplexBasic("BigInteger", "BigIntegerBasic")

generic_module("ComplexFast")
ComplexFast("Real",     "RealBasic")
ComplexFast("LongReal", "LongRealBasic")
ComplexFast("Extended", "ExtendedBasic")


generic_module("ComplexTrans")
ComplexTrans("Real",     "RealComplexFast",      "RealBasic",	  "RealTrans")
ComplexTrans("LongReal", "LongRealComplexFast", "LongRealBasic", "LongRealTrans")
ComplexTrans("Extended", "ExtendedComplexFast", "ExtendedBasic", "ExtendedTrans")

generic_module("ComplexFmtLex")
ComplexFmtLex("Real",	    "RealComplexBasic",       "RealBasic",       "RealFmtLex")
ComplexFmtLex("LongReal",   "LongRealComplexBasic",   "LongRealBasic",   "LongRealFmtLex")
ComplexFmtLex("Extended",   "ExtendedComplexBasic",   "ExtendedBasic",   "ExtendedFmtLex")
ComplexFmtLex("Integer32",  "Integer32ComplexBasic",  "Integer32Basic",  "Integer32FmtLex")
ComplexFmtLex("BigInteger", "BigIntegerComplexBasic", "BigIntegerBasic", "BigIntegerFmtLex")
