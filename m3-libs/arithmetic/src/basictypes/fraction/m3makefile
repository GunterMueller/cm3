import("libm3")

template("fraction")


generic_module("FractionBasic")
FractionBasic("Integer32",     "Integer32Basic")
%FractionBasic("BigInteger",    "BigIntegerBasic")

generic_module("FractionFmtLex")
FractionFmtLex("Integer32",  "Integer32FractionBasic",  "Integer32Basic",  "Integer32FmtLex")
%FractionFmtLex("BigInteger", "BigIntegerFractionBasic", "BigIntegerBasic", "BigIntegerFmtLex")
