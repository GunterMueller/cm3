import("libm3")

template("vector")


generic_module("Vector")
generic_module("VectorRep")

generic_module("VectorFast")
generic_module("VectorBasic")

Vector("Real",     "Fast", "Real",     "Basic")
Vector("LongReal", "Fast", "LongReal", "Basic")
Vector("Extended", "Fast", "Extended", "Basic")

Vector("RealComplex",        "Basic", "RealComplex",        "Fast")
Vector("LongRealComplex",    "Basic", "LongRealComplex",    "Fast")
Vector("ExtendedComplex",    "Basic", "ExtendedComplex",    "Fast")
Vector("Integer32",          "Basic", "Integer32",          "Basic")
Vector("BigInteger",         "Basic", "BigInteger",         "")
Vector("BigIntegerFraction", "Basic", "BigIntegerFraction", "")

generic_module("VectorTrans")
VectorTrans("Real",            "Real",     "",        "Basic")
VectorTrans("LongReal",        "LongReal", "",        "Basic")
VectorTrans("Extended",        "Extended", "",        "Basic")
VectorTrans("RealComplex",     "Real",     "Complex", "Fast")
VectorTrans("LongRealComplex", "LongReal", "Complex", "Fast")
VectorTrans("ExtendedComplex", "Extended", "Complex", "Fast")

generic_module("VectorFmtLex")
VectorFmtLex("Real")
VectorFmtLex("LongReal")
VectorFmtLex("Extended")
VectorFmtLex("LongRealComplex")
VectorFmtLex("Integer32")
VectorFmtLex("BigInteger")
