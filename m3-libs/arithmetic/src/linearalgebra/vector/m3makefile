import("libm3")

template("vector")


generic_module("VectorFast")
VectorFast("Real",     "Real",     "Basic")
VectorFast("LongReal", "LongReal", "Basic")
VectorFast("Extended", "Extended", "Basic")

generic_module("VectorBasic")
generic_module("VectorSupport")
VectorBasic("RealComplex",        "RealComplex",        "Fast")
VectorBasic("LongRealComplex",    "LongRealComplex",    "Fast")
VectorBasic("ExtendedComplex",    "ExtendedComplex",    "Fast")
VectorBasic("Integer32",          "Integer32",          "Basic")
VectorBasic("BigInteger",         "BigInteger",         "Basic")
VectorBasic("BigIntegerFraction", "BigIntegerFraction", "Basic")

generic_module("VectorTrans")
VectorTrans("Real",            "Real",     "",        "Basic", "Fast")
VectorTrans("LongReal",        "LongReal", "",        "Basic", "Fast")
VectorTrans("Extended",        "Extended", "",        "Basic", "Fast")
VectorTrans("RealComplex",     "Real",     "Complex", "Fast",  "Basic")
VectorTrans("LongRealComplex", "LongReal", "Complex", "Fast",  "Basic")
VectorTrans("ExtendedComplex", "Extended", "Complex", "Fast",  "Basic")

generic_module("VectorFmtLex")
VectorFmtLex("Real",            "Fast")
VectorFmtLex("LongReal",        "Fast")
VectorFmtLex("Extended",        "Fast")
VectorFmtLex("LongRealComplex", "Basic")
VectorFmtLex("Integer32",       "Basic")
VectorFmtLex("BigInteger",      "Basic")
