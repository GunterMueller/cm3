%
%  Quake functions to instantiate fourier transform on generic float types.
%

readonly proc FourierTransform (nm, field, rfast, cfast) is
  local basic = nm & "FourierTransform"
  local rfield = field & rfast
  local rtrans = field & "Trans"
  local cfield = field & "Complex" & cfast
  local ctrans = field & "ComplexTrans"
  build_generic_intf (basic, "FourierTransform", [                cfield], VISIBLE)
  build_generic_impl (basic, "FourierTransform", [rfield, rtrans, cfield, ctrans])
end
