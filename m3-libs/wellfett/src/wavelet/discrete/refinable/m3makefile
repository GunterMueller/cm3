%RefinableFunc("LongRealComplex", "LongRealComplex", "Fast",  "LongRealComplex", "Fast",  "Basic",  "Basic", "Basic")
%RefinableFunc("ExtendedComplex", "ExtendedComplex", "Fast",  "ExtendedComplex", "Fast",  "Basic",  "Basic", "Basic")
