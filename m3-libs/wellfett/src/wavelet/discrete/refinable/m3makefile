
import("bilist")

template("refinablefunc")


generic_module("RefinableFunc")
RefinableFunc("Real",            "RealBasic",       "RealComplex")
RefinableFunc("LongReal",        "LongRealBasic",   "LongRealComplex")
% requires EigenSystem
%RefinableFunc("Extended",        "ExtendedBasic",   "ExtendedComplex")
%RefinableFunc("RealComplex",     "RealComplex",     "RealComplex")
%RefinableFunc("LongRealComplex", "LongRealComplex", "LongRealComplex")
%RefinableFunc("ExtendedComplex", "ExtendedComplex", "ExtendedComplex")

generic_module("RefinableSmooth")
RefinableSmooth("Real",            "RealBasic",       "RealComplex")
RefinableSmooth("LongReal",        "LongRealBasic",   "LongRealComplex")
