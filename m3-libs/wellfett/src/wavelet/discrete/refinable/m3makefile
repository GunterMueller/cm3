
template("refinablefunc")


generic_module("RefinableFunc")
RefinableFunc("Real",            "Real",            "Basic", "RealComplex",	"Fast",  "Fast",   "Basic", "Fast")
RefinableFunc("LongReal",        "LongReal",        "Basic", "LongRealComplex", "Fast",  "Fast",   "Basic", "Fast")
% requires EigenSystem
%RefinableFunc("Extended",        "Extended",        "Basic", "ExtendedComplex", "Fast",  "Fast",   "Basic", "Fast")
