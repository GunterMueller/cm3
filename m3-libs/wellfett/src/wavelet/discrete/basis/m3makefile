
template("daubechieswavelet")


generic_module("DaubechiesWavelet")
DaubechiesWavelet("Real",            "RealBasic",            "RealSignal")
DaubechiesWavelet("LongReal",        "LongRealBasic",        "LongRealSignal")
DaubechiesWavelet("Extended",        "ExtendedBasic",        "ExtendedSignal")
%DaubechiesWavelet("RealComplex",     "RealComplexFast",      "RealComplexSignal")
%DaubechiesWavelet("LongRealComplex", "LongRealComplexFast",  "LongRealComplexSignal")
%DaubechiesWavelet("ExtendedComplex", "ExtendedComplexFast",  "ExtendedComplexSignal")
