
template("daubechieswavelet")
template("bsplinewavelet")
template("filterbank")


generic_module("DaubechiesWavelet")
DaubechiesWavelet("Real",            "RealBasic")
DaubechiesWavelet("LongReal",        "LongRealBasic")
DaubechiesWavelet("Extended",        "ExtendedBasic")
%DaubechiesWavelet("RealComplex",     "RealComplex")
%DaubechiesWavelet("LongRealComplex", "LongRealComplex")
%DaubechiesWavelet("ExtendedComplex", "ExtendedComplex")

generic_module("BSplineWavelet")
BSplineWavelet("Real",            "RealBasic")
BSplineWavelet("LongReal",        "LongRealBasic")
BSplineWavelet("Extended",        "ExtendedBasic")
%BSplineWavelet("RealComplex",     "RealComplex")
%BSplineWavelet("LongRealComplex", "LongRealComplex")
%BSplineWavelet("ExtendedComplex", "ExtendedComplex")

generic_module("FilterBank")
%FilterBank("Real",            "RealBasic")
FilterBank("LongReal",        "LongRealBasic")
%FilterBank("Extended",        "ExtendedBasic")
%FilterBank("RealComplex",     "RealComplex")
FilterBank("LongRealComplex", "LongRealComplex")
%FilterBank("ExtendedComplex", "ExtendedComplex")

generic_module("DyadicFilterBank")
%DyadicFilterBank("Real",            "RealBasic")
DyadicFilterBank("LongReal",        "LongRealBasic")
%DyadicFilterBank("Extended",        "ExtendedBasic")
%DyadicFilterBank("RealComplex",     "RealComplex")
DyadicFilterBank("LongRealComplex", "LongRealComplex")
%DyadicFilterBank("ExtendedComplex", "ExtendedComplex")

