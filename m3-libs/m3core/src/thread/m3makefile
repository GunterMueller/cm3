include_dir ("Common")


if equal (OS_TYPE, "WIN32") or equal(TARGET, "NT386")
    include_dir ("WIN32")
else
    if (not defined("NOPTHREAD")) and USE_PTHREADS{TARGET}
        include_dir("PTHREAD")
    else
        include_dir (OS_TYPE)
    end
end
