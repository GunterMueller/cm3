if equal (OS_TYPE, "WIN32")
  include_dir ("WIN32")
else

include_dir ("Common")

if _UnixPieces contains TARGET
  local pieces = _UnixPieces {TARGET}
  foreach f in pieces  include_dir (f)  end
else
  error (format ("unix: unsupported platform: \"%s\"%s", TARGET, CR))
end
end


