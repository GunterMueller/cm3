readonly TARGET = "PPC_DARWIN"

proc FileExists(x) is
    return not stale(x, x)
end

SYSTEM_ASM = "/usr/libexec/gcc/darwin/ppc/as"
if not FileExists(SYSTEM_ASM)
    SYSTEM_ASM = "as"
end

include("PPC.common")
include("Darwin.common")
