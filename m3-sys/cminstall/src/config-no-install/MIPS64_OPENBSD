readonly TARGET = "MIPS64_OPENBSD"
readonly WORD_SIZE = "64BITS"
readonly GNU_PLATFORM = "mips64-openbsd"

readonly m3back_flags = "-gstabs+ -mabi=64 -fPIC -quiet"
readonly SYSTEM_CC = "gcc -g -mabi=64 -fPIC"
readonly SYSTEM_ASM = "as"

%
% newer platforms never needed these workarounds for
% bootstrapping with older builds
%
readonly proc GetGcWrapFlags() is return "" end
readonly proc FixM3GcDefs(x) is return x end
readonly proc GetM3BackFlag(x) is return x end

GNU_MAKE = "gmake"

include("Unix.common")
