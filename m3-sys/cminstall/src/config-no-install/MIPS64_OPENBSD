readonly TARGET = "MIPS64_OPENBSD"
readonly TARGET_ENDIAN = "BIG"
readonly TARGET_OS = "OPENBSD"
readonly WORD_SIZE = "64BITS"

% -funwind-tables causes internal compiler errors
% -mabi=64 presumably substitutes for -m64
m3back_flags = "-gstabs+ -mabi=64 -fPIC"
SYSTEM_CC = "gcc -g -mabi=64 -fPIC"
readonly SYSTEM_ASM = "as"

GNU_MAKE = "gmake"

include("Unix.common")
